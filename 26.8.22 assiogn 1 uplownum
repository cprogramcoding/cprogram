#include<stdio.h>
void main()
{ 
 char s[50];
 int i,t=0,k=0,b=0;
 printf("enter string");
 gets(s);
 for(i=0;s[i]!='\0';i++)
 {
  if(isupper(s[i]))
  {  
   t++;
  }
  else
  {
    if(islower(s[i]))
    {
      k++;
    }
    else
    b++;
  }
 }
 printf("Upper %d\n",t);
 printf("Lower %d\n",k);
 printf("Numbers %d\n",b);
}



out put
enter string naga123PENNAMMA
Upper 8
Lower 4
Numbers 3
