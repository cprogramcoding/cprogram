#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans;
    if(a>b&&a>c&&a>d)
    ans=a;
    if(a<b&&b>c&&b>d)
    ans=b;
    if(a<c&&b<c&&c>d)
    ans=c;
    if(a<d&&b<d&&c<d)
    ans=d;
    printf("%d", ans);
    
    return 0;
}
