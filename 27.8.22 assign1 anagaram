#include<stdio.h>
#include<string.h>
int main()
{
 	char s1[10],s2[10];
	int i,j,temp,n,c=0;
	printf("enter two strings:");
	gets(s10;
	gets(s2);
	n=strlen(s1);
	for(i=0;i<n-1;i++)
	{
		for(j=0;j<n-i-1;j++)
		{
			if(s1[j]>s1[j+1])
			{
				temp=s1[j];
				s1[j]=s1[j+1];
				s1[j+1]=temp;
			}
		}
	}
	n=strlen(s2)
	for(i=0;i<n-1;i++)
	{
		for(j=0;j<n-i-1;j++)
		{
			if(s2[j]>s2[j+1])
			{
				temp=s2[j];
				s2[j]=s2[j+1];
				s2[j+1]=temp;
			}
		}
	}
	for(i=0;s1[i]!='\0';i++)
	{
		if(s1[i]==s2[i])
		c++;
	}
	if(c==n)
	printf("Anagarams");
	else
	printf("Not anagarams");
}
